      <Components isList="true" count="10">
        <Text27 Ref="29" isKey="true" type="Text">
          <AllowHtmlTags>True</AllowHtmlTags>
          <Brush>Transparent</Brush>
          <ClientRectangle>0.8,10.4,17.2,0.6</ClientRectangle>
