        <Text12 Ref="66" isKey="true" type="Text">
          <AllowHtmlTags>True</AllowHtmlTags>
          <Brush>Transparent</Brush>
          <ClientRectangle>0.8,23.4,17.2,1.2</ClientRectangle>
