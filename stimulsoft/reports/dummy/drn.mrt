          <ClientRectangle>0.8,10.2,18.2,1.2</ClientRectangle>
          <Conditions isList="true" count="0"/>
          <Font>Calibri,12</Font>
          <Guid>744527436451ddc6b1a2187db2791350</Guid>
          <HorAlignment>Width</HorAlignment>
          <Margins>0,0,0,0</Margins>
          <Name>Text57</Name>
          <Page isRef="122"/>
          <Parent isRef="122"/>
