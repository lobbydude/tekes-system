        <Text55 isKey="true" Ref="125" type="Text">
          <Brush>Transparent</Brush>
          <ClientRectangle>1.6,7.2,17.4,1.2</ClientRectangle>
          <Conditions isList="true" count="0"/>
          <Font>Calibri,12</Font>
          <Guid>e366ced823eb55b9dc6b187ac721ce0a</Guid>
          <HorAlignment>Width</HorAlignment>
          <Margins>0,0,0,0</Margins>
          <Name>Text55</Name>
          <Page isRef="122"/>
          <Parent isRef="122"/>
          <Text>e.  Your duty to safeguard and not disclose Confidential Information will survive the expiration or termination of this Agreement and/or your employment with the Company.</Text>
          <TextBrush>Black</TextBrush>
          <TextOptions>HotkeyPrefix=None, LineLimit=False, RightToLeft=False, Trimming=None, WordWrap=True, Angle=0, FirstTabOffset=40, DistanceBetweenTabs=20,</TextOptions>
          <Type>Expression</Type>
        </Text55>
        <Text56 isKey="true" Ref="126" type="Text">
          <Brush>Transparent</Brush>
